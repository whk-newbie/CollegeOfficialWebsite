AA,CAEM,KAFN,EAEM,UAFN,EAEM,CADJ,mBAAA,CAAwD,KAAxD,EAAwD;AAAnD,IAAA,SAA0B,EAAlB,IAAA,CAAA,IAAA,CAAK,YAAsC;AAAxB,IAAA,KAAK,EAAC;AAAkB,GAAxD,E,IAAA,E;;AAAA,I,UAAA,CACI,CAFN,C,IAGgB,IAAA,CAAA,IAAA,CAAK,I,kBAArB,mBAAA,CAEM,KAFN,EAEM,UAFN,EAEM,UAFN,C,mBAGA,mBAAA,CAGM,KAHN,EAGM,WAHN,EAGM,CAFF,YAAA,CAAqD,mBAArD,EAAqD;AAA3C,IAAA,WAAW,EAAC;AAA+B,GAArD,CAEE,EADF,YAAA,CAAsF,oBAAtF,EAAsF;AAA3E,IAAA,KAAK,EAAC,UAAqE;AAA1D,IAAA,IAAI,EAAC,SAAqD;AAA1C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,OAAA,CAAQ,EAAR,CAAU,CAAA,CAAV,CAAF;AAAA,KAAA;AAAqC,GAAtF,E;sBAAmE;AAAA,aAAO,C,WAAA,CAAP;AAAA,K;;;;GAAnE,CACE,CAHN,C,CAII,CAfN,C,qCAgBI,CAjBN,C,yBAAuC,IAAA,CAAA,O,KAkBvC,YAAA,CAAS,iBAAT,C","sourcesContent":["<template>\r\n  <Header/>\r\n  <div class=\"grid-container\" v-loading=\"loading\">\r\n    <div v-if=\"news!==null\">\r\n      <h1 id=\"title\">{{ news.title }}</h1>\r\n      <p id=\"subtitle\">\r\n        发布时间: {{ formatted_time(news.pub_time) }}\r\n      </p>\r\n      <div v-if=\"news.content_html\">\r\n        <div v-html=\"news.content_html\" class=\"news-body\"></div>\r\n      </div>\r\n      <div v-else-if=\"news.file\">\r\n        <div>文件展示</div>\r\n      </div>\r\n      <div v-else>\r\n          <el-empty description=\"加载失败！请联系网站管理员，点击下方按钮，返回上一页\" />\r\n          <el-button class=\"back-btn\" type=\"primary\" @click=\"$router.go(-1)\">Primary</el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <Footer/>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Header from \"@/components/Header\";\r\nimport Footer from \"@/components/Footer\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"InfosDetail\",\r\n  components: {Header, Footer},\r\n  data: function () {\r\n    return {\r\n      loading: false,\r\n      news: null,\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loading = true;\r\n    this.getData()\r\n  },\r\n  methods: {\r\n    getData() {\r\n      axios\r\n          .get('/api/infos/' + this.$route.params.id)\r\n          .then(response => (this.news = response.data))\r\n      this.loading = false;\r\n    },\r\n    formatted_time: function (time) {\r\n      const date = new Date(time);\r\n      // return date.toLocaleDateString()\r\n      return date.getFullYear() + '-' + (date.getMonth() + 1) + '-' + date.getDate() + ' ' + date.getHours() + ':' + date.getMinutes() + ':' + date.getSeconds()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.grid-container {\r\n  margin: 1% 2% 2% 10%;\r\n  height: auto;\r\n  width: 80%;\r\n  box-shadow: 4px 4px 15px #dad9d9;\r\n}\r\n\r\n\r\n#title {\r\n  margin-top: 5%;\r\n  text-align: center;\r\n  font-size: x-large;\r\n}\r\n\r\n#subtitle {\r\n  text-align: center;\r\n  color: gray;\r\n  font-size: small;\r\n}\r\n\r\n.news-body {\r\n  margin-left: 10%;\r\n  margin-right: 10%;\r\n}\r\n.back-btn {\r\n  margin-right: 10%;\r\n  margin-left: 45%;\r\n  margin-bottom: 2%;\r\n}\r\n</style>\r\n\r\n<style>\r\n.news-body p img {\r\n\r\n  max-width: 100%;\r\n  border-radius: 50px;\r\n  box-shadow: gray 0 0 20px;\r\n}\r\n\r\n.toc ul {\r\n  list-style-type: none;\r\n}\r\n\r\n.toc a {\r\n  color: gray;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}