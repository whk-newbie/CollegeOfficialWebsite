,MAAA,CAAA,IAAA,CAAK,YAAL,KAAiB,E,kBAA5B,mBAAA,CAEM,KAFN,EAEM,UAFN,EAEM,CADJ,mBAAA,CAAwD,KAAxD,EAAwD;AAAnD,IAAA,SAA0B,EAAlB,MAAA,CAAA,IAAA,CAAK,YAAsC;AAAxB,IAAA,KAAK,EAAC;AAAkB,GAAxD,E,IAAA,E;;AAAA,I,UAAA,CACI,CAFN,C,IAGgB,MAAA,CAAA,IAAA,CAAK,IAAL,KAAS,E,kBAAzB,mBAAA,CAOM,KAPN,EAOM,UAPN,EAOM,CANO,QAAA,CAAA,K,kBAAX,mBAAA,CAEM,KAFN,EAEM,UAFN,EAEM,CADJ,YAAA,CAA4B,cAA5B,EAA4B;AAAtB,IAAA,GAAG,EAAE,MAAA,CAAA,IAAA,CAAK;AAAY,GAA5B,E,IAAA,E;;AAAA,I,OAAA,CACI,CAFN,C,mBAGA,mBAAA,CAEM,KAFN,EAEM,WAFN,EAEM,CADJ,YAAA,CAA+C,oBAA/C,EAA+C;AAAnC,IAAA,OAAK,EAAE,QAAA,CAAA;AAA4B,GAA/C,EAA2B;sBAAE;AAAA,aAAM,C,WAAA,CAAN;AAAA,K,CAAF;;;;AAAA,GAA3B,E;;AAAA,I,WAAA,CACI,CAFN,C,CAGI,CAPN,C,mBAQA,mBAAA,CAGM,KAHN,EAGM,WAHN,EAGM,CAFJ,YAAA,CAAoD,mBAApD,EAAoD;AAA1C,IAAA,WAAW,EAAC;AAA8B,GAApD,CAEI,EADJ,YAAA,CAAsF,oBAAtF,EAAsF;AAA3E,IAAA,KAAK,EAAC,UAAqE;AAA1D,IAAA,IAAI,EAAC,SAAqD;AAA1C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,OAAA,CAAQ,EAAR,CAAU,CAAA,CAAV,CAAF;AAAA,KAAA;AAAqC,GAAtF,E;sBAAmE;AAAA,aAAO,C,WAAA,CAAP;AAAA,K;;;;GAAnE,CACI,CAHN,C,CAII,CAhBN,CAiBI,CAtBN,C,qCAuBI,CAxBN,C,yBAAuC,IAAA,CAAA,O,KAyBvC,YAAA,CAAS,iBAAT,C","sourcesContent":["<template>\r\n  <Header/>\r\n  <div class=\"grid-container\" v-loading=\"loading\">\r\n    <div v-if=\"news!==null\">\r\n      <h1 id=\"title\">{{ news.title }}</h1>\r\n      <p id=\"subtitle\">\r\n        发布时间: {{ DetailTime(news.create_time) }}\r\n      </p>\r\n      <div class=\"content_body\">\r\n        <div v-if=\"news.content_html !== ''\">\r\n          <div v-html=\"news.content_html\" class=\"news-body\"></div>\r\n        </div>\r\n        <div v-else-if=\"news.file !=='' \">\r\n          <div v-if=\"judge\">\r\n            <pdf :src=\"news.file\"></pdf>\r\n          </div>\r\n          <div v-else>\r\n            <el-button @click=\"openfile\">点此查看文件</el-button>\r\n          </div>\r\n        </div>\r\n        <div v-else>\r\n          <el-empty description=\"加载失败！请联系网站管理员，点击下方按钮，返回上一页\"/>\r\n          <el-button class=\"back-btn\" type=\"primary\" @click=\"$router.go(-1)\">Primary</el-button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <Footer/>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Header from \"@/components/Header\";\r\nimport Footer from \"@/components/Footer\";\r\nimport pdf from 'vue3-pdf'\r\nimport DetailTime from \"@/composables/DetailTime\";\r\nimport getPages from \"@/composables/GetPageData\";\r\nimport {ref} from 'vue'\r\nimport {useRoute} from 'vue-router'\r\n\r\nexport default {\r\n  name: \"InfosDetail\",\r\n  components: {Header, Footer, pdf},\r\n  setup() {\r\n    const news = ref('')\r\n    const route = useRoute();\r\n    getPages('/api/news/', news, route)\r\n    return {\r\n      news,\r\n      DetailTime\r\n    }\r\n  },\r\n  data: function () {\r\n    return {\r\n      pdfstring: 'pdf',\r\n\r\n    }\r\n  },\r\n  methods: {\r\n    openfile() {\r\n      var path = this.news.file\r\n      window.open(path)\r\n      // window.open(\"https://view.officeapps.live.com/op/view.aspx?src=\" + path)\r\n    },\r\n    judge() {\r\n      if (this.news.file.substring(this.news.file.length - 3) === 'pdf') {\r\n        return true\r\n      } else {\r\n        return false\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.grid-container {\r\n  margin: 1% 2% 2% 10%;\r\n  height: auto;\r\n  width: 80%;\r\n  box-shadow: 4px 4px 15px #dad9d9;\r\n}\r\n\r\n\r\n#title {\r\n  margin-top: 5%;\r\n  text-align: center;\r\n  font-size: x-large;\r\n}\r\n\r\n#subtitle {\r\n  text-align: center;\r\n  color: gray;\r\n  font-size: small;\r\n}\r\n\r\n.news-body {\r\n  margin-left: 10%;\r\n  margin-right: 10%;\r\n}\r\n\r\n.content_body {\r\n  margin-right: 5%;\r\n  margin-left: 5%;\r\n}\r\n\r\n.back-btn {\r\n  margin-right: 10%;\r\n  margin-left: 45%;\r\n  margin-bottom: 2%;\r\n}\r\n</style>\r\n\r\n<style>\r\n.news-body p img {\r\n\r\n  max-width: 100%;\r\n  border-radius: 50px;\r\n  box-shadow: gray 0 0 20px;\r\n}\r\n\r\n.toc ul {\r\n  list-style-type: none;\r\n}\r\n\r\n.toc a {\r\n  color: gray;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}