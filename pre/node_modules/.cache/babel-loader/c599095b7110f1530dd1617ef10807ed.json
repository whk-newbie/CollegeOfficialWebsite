;;AAAA,I,UAAA,CACI,CANN,C,yBAAuC,IAAA,CAAA,O,GAOnC,CARN,C,uCASA,YAAA,CAAS,iBAAT,C","sourcesContent":["<template>\r\n  <Header/>\r\n  <div v-if=\"news!==null\">\r\n    <div class=\"grid-container\" v-loading=\"loading\">\r\n      <h1 id=\"title\">{{ pages.title }}</h1>\r\n      <p id=\"subtitle\">\r\n        发布时间: {{ formatted_time(pages.created) }}\r\n      </p>\r\n      <div v-html=\"pages.content_html\" class=\"news-body\"></div>\r\n    </div>\r\n  </div>\r\n  <Footer/>\r\n</template>\r\n\r\n<script>\r\nimport Header from \"@/components/Header\";\r\nimport Footer from \"@/components/Footer\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"TmoralityDetail\",\r\n  components: {Header, Footer},\r\n  data: function () {\r\n    return {\r\n      loading: false,\r\n      pages: '',\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loading = true;\r\n    this.getData()\r\n  },\r\n  methods: {\r\n    getData() {\r\n      axios\r\n          .get('/api/Tmorality/' + this.$route.params.id)\r\n          .then(response => (this.pages = response.data))\r\n      this.loading = false;\r\n    },\r\n    formatted_time: function (time) {\r\n      const date = new Date(time);\r\n      // return date.toLocaleDateString()\r\n      return date.getFullYear() + '-' + (date.getMonth() + 1) + '-' + date.getDate() + ' ' + date.getHours() + ':' + date.getMinutes() + ':' + date.getSeconds()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.grid-container {\r\n  margin: 1% 2% 2% 10%;\r\n  height: auto;\r\n  width: 80%;\r\n  box-shadow: 4px 4px 15px #dad9d9;\r\n}\r\n\r\n\r\n#title {\r\n  margin-top: 5%;\r\n  text-align: center;\r\n  font-size: x-large;\r\n  padding: 20px;\r\n}\r\n\r\n#subtitle {\r\n  text-align: center;\r\n  color: gray;\r\n  font-size: small;\r\n}\r\n\r\n.news-body {\r\n  margin-left: 10%;\r\n  margin-right: 10%;\r\n}\r\n</style>\r\n<style>\r\n.news-body p img {\r\n  max-width: 100%;\r\n  border-radius: 50px;\r\n  box-shadow: gray 0 0 20px;\r\n}\r\n